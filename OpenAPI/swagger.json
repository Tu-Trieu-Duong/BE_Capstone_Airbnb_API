  "info": {
    "title": "Airbnb Clone API",
    "description": "API for Airbnb clone",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": { "scheme": "bearer", "bearerFormat": "JWT", "type": "http" }
    },
    "schemas": {
      "SignupDto": {
        "type": "object",
        "properties": {
          "email": { "type": "string", "example": "tutrieuduong@gmail.com" },
          "password": { "type": "string", "example": "123aA@" },
          "name": { "type": "string", "example": "Nguyen Van A" },
          "phone": { "type": "string", "example": "0123456789" },
          "birth_day": { "type": "string", "example": "2000-01-01" },
          "gender": { "type": "boolean", "example": true }
        },
        "required": [
          "email",
          "password",
          "name",
          "phone",
          "birth_day",
          "gender"
        ]
      },
      "SigninDto": {
        "type": "object",
        "properties": {
          "email": { "type": "string", "example": "tutrieuduong@gmail.com" },
          "password": { "type": "string", "example": "123aA@" }
        },
        "required": ["email", "password"]
      },
      "RefreshTokenDto": {
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string",
            "example": "your_refresh_token_here"
          }
        },
        "required": ["refreshToken"]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": { "type": "string", "example": "tutrieuduong@gmail.com" },
          "password": { "type": "string", "example": "123aA@" },
          "name": { "type": "string", "example": "Nguyen Van A" },
          "phone": { "type": "string", "example": "0123456789" },
          "birth_day": { "type": "string", "example": "2000-01-01" },
          "gender": {
            "type": "boolean",
            "description": "Giới tính: true = Nam, false = Nữ",
            "default": true
          },
          "role": {
            "type": "string",
            "enum": ["user", "admin"],
            "example": "user",
            "description": "Vai trò của người dùng"
          }
        },
        "required": [
          "email",
          "password",
          "name",
          "phone",
          "birth_day",
          "gender",
          "role"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "example": "Nguyen Van A" },
          "phone": { "type": "string", "example": "0123456789" },
          "birth_day": { "type": "string", "example": "2000-01-01" },
          "gender": {
            "type": "boolean",
            "description": "Giới tính: true = Nam, false = Nữ",
            "default": true
          },
          "role": {
            "type": "string",
            "enum": ["user", "admin"],
            "example": "user",
            "description": "Vai trò của người dùng"
          }
        },
        "required": ["name", "phone", "birth_day", "role"]
      },
      "CreateLocationDto": {
        "type": "object",
        "properties": {
          "name_location": { "type": "string", "example": "Ho Chi Minh City" },
          "province": { "type": "string", "example": "Ho Chi Minh" },
          "country": { "type": "string", "example": "Vietnam" }
        },
        "required": ["name_location", "province", "country"]
      },
      "UpdateLocationDto": {
        "type": "object",
        "properties": {
          "name_location": { "type": "string", "example": "Ho Chi Minh City" },
          "province": { "type": "string", "example": "Ho Chi Minh" },
          "country": { "type": "string", "example": "Vietnam" }
        }
      },
      "CreateRoomDto": {
